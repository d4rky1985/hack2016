<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\ListsToProducts" table="lists_to_products">
    <indexes>
      <index name="FK_lists_to_products_shopping_list" columns="shopping_list_id"/>
      <index name="FK_lists_to_products_products" columns="product_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="shoppingList" target-entity="ShoppingList" fetch="LAZY">
      <join-columns>
        <join-column name="shopping_list_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="product" target-entity="Products" fetch="LAZY">
      <join-columns>
        <join-column name="product_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
